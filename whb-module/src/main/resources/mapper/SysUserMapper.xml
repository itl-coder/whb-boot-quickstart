<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.whb.mapper.SysUserMapper">

    <resultMap id="BaseResultMap" type="com.example.whb.domain.SysUser">
